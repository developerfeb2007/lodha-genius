{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Slug": {
      "type": "string"
    },
    "PublishedDate": {
      "type": "date"
    },
    "DesktopBanner": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "MobileBanner": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "Content": {
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
