{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "countryCode": {
      "type": "string"
    },
    "mobile": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "otp": {
      "type": "integer",
      "private": true
    },
    "SchoolDetails": {
      "displayName": "School Details",
      "type": "component",
      "repeatable": false,
      "component": "user.school-details"
    },
    "AcademicRecord": {
      "displayName": "Academic Record",
      "type": "component",
      "repeatable": false,
      "component": "user.academic-record"
    },
    "GuardianDetails": {
      "displayName": "Guardian Details",
      "type": "component",
      "repeatable": false,
      "component": "user.guardian-details"
    },
    "PersonalContactDetails": {
      "displayName": "Personal",
      "type": "component",
      "repeatable": false,
      "component": "user.personal"
    },
    "OtherInformation": {
      "displayName": "Other Info",
      "type": "component",
      "repeatable": false,
      "component": "user.other-info"
    },
    "Documents": {
      "type": "component",
      "repeatable": false,
      "component": "user.uploads"
    },
    "registrationNumber": {
      "type": "string",
      "unique": true
    },
    "ApplicationStep": {
      "type": "integer",
      "default": 1,
      "required": true,
      "max": 7,
      "min": 1
    },
    "TestingStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Submitted",
        "Approved",
        "Rejected"
      ],
      "default": "Pending",
      "required": true
    },
    "InterviewStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Submitted",
        "Approved",
        "Rejected"
      ],
      "default": "Pending",
      "required": true
    },
    "PostApplicationStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Submitted",
        "Approved",
        "Rejected"
      ],
      "default": "Pending",
      "required": true
    },
    "ApplicationStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Submitted",
        "Approved",
        "Rejected"
      ]
    }
  }
}
